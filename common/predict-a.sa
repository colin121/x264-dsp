            ADD             dc_1,       v_1,        dc_1
            ADD             dc_0,       v_2,        dc_0
            ADD             dc_1,       v_3,        dc_1
            ; load vertical pixels (4, 5, 6, 7)
            LDBU            *pix_lf++[pix_1x], v_0
            LDBU            *pix_lf++[pix_1x], v_1
            LDBU            *pix_lf++[pix_1x], v_2
            LDBU            *pix_lf++[pix_1x], v_3
            ADD             dc_0,       v_0,        dc_0
            ADD             dc_1,       v_1,        dc_1
            ADD             dc_0,       v_2,        dc_0
            ADD             dc_1,       v_3,        dc_1
            ; load vertical pixels (8, 9, A, B)
            LDBU            *pix_lf++[pix_1x], v_0
            LDBU            *pix_lf++[pix_1x], v_1
            LDBU            *pix_lf++[pix_1x], v_2
            LDBU            *pix_lf++[pix_1x], v_3
