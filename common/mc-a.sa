		.sect ".text:_pixel_avg_w8o_ti"
        .global _pixel_avg_w8o_ti
_pixel_avg_w8o_ti .cproc dst, i_dst_stride, src1, i_src1_stride, src2, i_src2_stride, i_width, i_height
