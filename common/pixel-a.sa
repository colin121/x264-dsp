            ADD2            pix_X6,     k_2_2,      pix_X6
            SHR2            pix_10,     2,          pix_10
            SHR2            pix_32,     2,          pix_32
            SHR2            pix_54,     2,          pix_54
            SHR2            pix_X6,     2,          pix_X6
            SPACKU4         pix_32,     pix_10,     pix2_3_3210
            SPACKU4         pix_54,     pix_32,     pix2_1_3210
