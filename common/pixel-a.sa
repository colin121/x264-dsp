            UNPKHU4			pix0_3_3210, pix0_3_32
            UNPKLU4			pix0_3_3210, pix0_3_10

            ; load horizontal and vertical pixels
            ;*  M  A  B  C  D  E  F  G  H *;
            ;*  I  a  b  c  d             *;
            ;*  J  e  f  g  h             *;
            ;*  K  i  j  k  l             *;
            ;*  L  m  n  o  p             *;
