            SHL             mv_1_32,    4,          mv_1_32
            SHL             mv_2_32,    4,          mv_2_32
            SHL             mv_3_32,    4,          mv_3_32
            OR              mv_0_32,    mv_0_10,    mv_0_3210
            OR              mv_1_32,    mv_1_10,    mv_1_3210
