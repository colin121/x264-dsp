            DPACK2          bs_1_10,   bs_0_10,  bs_lo_11:bs_lo_00
            DPACK2          bs_3_10,   bs_2_10,  bs_hi_11:bs_hi_00
            DPACK2          bs_1_32,   bs_0_32,  bs_lo_33:bs_lo_22
            DPACK2          bs_3_32,   bs_2_32,  bs_hi_33:bs_hi_22
            PACKL4          bs_hi_00,  bs_lo_00, bs_0_3210
